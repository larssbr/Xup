<h1>Listing communities</h1>

<table>
  <thead>
    <tr>
      <th>Me</th>

      <p>Hello</p>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @communities.each do |community| %>
      <tr>
        <td><%= community.me %></td>
        <td><%= link_to 'Show', community %></td>
        <td><%= link_to 'Edit', edit_community_path(community) %>
        <button type="button" class="btn btn-info">Edit</button></td>

        <td><%= link_to 'Destroy', community, method: :delete, data: { confirm: 'Are you sure?' } %>
          <button type="button" class="btn btn-danger">Destroy</button></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Community', new_community_path %>
